{
  "name": "umbrella-chat",
  "version": "1.0.0",
  "description": "Encrypted chat for you, and your squad.",
  "main": "server/index.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run test:server",
    "test:server": "jest --config=server.jest.config.json",
    "watch": "nodemon server/main.js | bunyan -o short",
    "standard": "standard --fix",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "client-start": "webpack-dev-server --mode development --open",
    "client-build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimtheStew/umbrella-chat.git"
  },
  "keywords": [
    "umbrella"
  ],
  "author": "Timothy R. Stewart",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimtheStew/umbrella-chat/issues"
  },
  "homepage": "https://github.com/TimtheStew/umbrella-chat#readme",
  "dependencies": {
    "apollo-server-express": "^2.4.8",
    "app-root-path": "^2.1.0",
    "bunyan": "^1.8.12",
    "config": "^3.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "jest": "^21.2.1",
    "jest-json-reporter": "^1.2.2",
    "jsonwebtoken": "^8.5.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "path": "^0.12.7",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
